<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:fresco="http://schemas.android.com/apk/com.facebook.drawee" xmlns:internal="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/corner_radius"/>
            <solid android:color="#f8f8f8"/>
            <stroke android:width="0.5dp" android:color="@color/gray_light_1"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="0.5dp" android:color="@color/gray_light_1"/>
            <corners android:radius="@dimen/corner_radius"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
</selector>
