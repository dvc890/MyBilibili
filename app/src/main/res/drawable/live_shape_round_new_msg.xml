<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:fresco="http://schemas.android.com/apk/com.facebook.drawee" xmlns:internal="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:top="1dp" android:right="1dp">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#01000000"/>
                    <corners android:radius="90dp"/>
                    <padding android:left="5px" android:top="5px" android:right="5px" android:bottom="5px"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#02000000"/>
                    <corners android:radius="90dp"/>
                    <padding android:left="3px" android:top="3px" android:right="3px" android:bottom="3px"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#03000000"/>
                    <corners android:radius="90dp"/>
                    <padding android:left="4px" android:top="4px" android:right="4px" android:bottom="4px"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#04000000"/>
                    <corners android:radius="90dp"/>
                    <padding android:left="3px" android:top="3px" android:right="3px" android:bottom="3px"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#05000000"/>
                    <corners android:radius="90dp"/>
                    <padding android:left="2px" android:top="2px" android:right="2px" android:bottom="2px"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#06000000"/>
                    <corners android:radius="90dp"/>
                    <padding android:left="1px" android:top="1px" android:right="1px" android:bottom="1px"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/theme_color_live_bg_new_msg" android:endColor="@color/theme_color_live_bg_new_msg" android:angle="270"/>
            <corners android:radius="90dp"/>
        </shape>
    </item>
</layer-list>
