<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:fresco="http://schemas.android.com/apk/com.facebook.drawee" xmlns:internal="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/corner_radius"/>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="1dp" android:color="@color/gray_dark"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/corner_radius"/>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="1dp" android:color="@color/theme_color_primary_dark"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/corner_radius"/>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="1dp" android:color="@color/theme_color_primary"/>
        </shape>
    </item>
</selector>
